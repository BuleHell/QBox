	background: transparent;
	border: none;
	color: #ffffff;
	font-size: 18px;
}
QPushButton#btnMin:hover {

	background: #569000;
}


/*关闭按钮*/
QPushButton#btnClose {
	background: transparent;
	border: none;
	color: #ffffff;
	font-size: 18px;
}
QPushButton#btnClose:hover {
	background: red;
}


/*注册按钮*/
QPushButton#btnLogin {
	border: 1px solid #c9c9c9;
	background: #569082;
	color:#ffffff;
	font-family: &quot;微软雅黑&quot;;
	font-size: 15px;
}
QPushButton#btnLogin:hover {
	border: 1px solid #c9c9c9;
	background: #ff9032;
	color:#ffffff;
	font-family: &quot;微软雅黑&quot;;
	font-size: 15px;
   
}
QPushButton#btnLogin:pressed {
	border:9px solid #c9c9c9;
	background: #569082;
	color:#ffffff;
	font-family: &quot;微软雅黑&quot;;
	font-size: 20px;
   
}


/*记住密码复选框*/
QCheckBox::indicator 
{
	width: 15px;
 	height: 15px;
	margin-right:5px;
}
QCheckBox#cbkAuto {
	font-family: &quot;微软雅黑&quot;;
	font-size: 15px;
	color: rgb(46, 44, 45);
}
QCheckBox#cbkAuto:hover {
	font-family: &quot;微软雅黑&quot;;
	font-size: 15px;
	
	color: rgb(0, 206, 0);
}
QCheckBox#cbkAuto:checked {
	font-family: &quot;微软雅黑&quot;;
	font-size: 15px;
	
	color: rgb(0, 170, 127);
}
/*输入框全局设置*/
QLineEdit {
	border: none;    
	font: 75 15pt &quot;微软雅黑&quot;;
	background: transparent;
}
/*输入用户名父控件*/
QWidget#widgetUser {
	border: 1px solid #c9c9c9;

}

/*输入密码父控件*/
QWidget#widgetPassword {
	border: 1px solid #c9c9c9;
}





